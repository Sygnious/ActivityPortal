<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    <!-- Navigation rules are ordered alphabetically-->
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>activityCancelled</from-outcome>
            <to-view-id>/activityCancelled.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>activityIndex</from-outcome>
            <to-view-id>/activityIndex.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>activityList</from-outcome>
            <to-view-id>/activityList.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>activityOne</from-outcome>
            <to-view-id>/activityOne.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>activitySearch</from-outcome>
            <to-view-id>/activitySearch.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>activitySignedOn</from-outcome>
            <to-view-id>/activitySignedOn.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <naviation-rule>
        <navigation-rule>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/error.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    </naviation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>interestList</from-outcome>
            <to-view-id>/interestList.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>mainPage</from-outcome>
            <to-view-id>/mainPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
</faces-config>
